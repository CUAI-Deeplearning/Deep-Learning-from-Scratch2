    "# repeat 노드\n",
    "import numpy as np \n",
    "D, N = 8,7\n",
    "x=np.random.randn(1,D)\n",
    "y = np.repeat(x, N, axis =0)  #axis = 0 행 방향으로 N개 \n",
    "dy = np.random.randn(N,D) \n",
    "dx = np.sum(dy, axis=0, keepdims = True)\n",
    "\n",
    "print(dy)\n",
    "print(dx)"
   ]
